		{
			"name": "Login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"//Muchi 1 Admin",
							"//Muchi 2 User",
							"//Muchi 3 Host",
							"//Muchi 4 Host",
							"//Muchi 5 User",
							"",
							"const jsonResponse = pm.response.json();",
							"",
							"if (jsonResponse.token && jsonResponse.role) {",
							"    const token = jsonResponse.token;",
							"    const role = jsonResponse.role;",
							"",
							"    if (role === \"admin\") {",
							"        pm.collectionVariables.set(\"token_admin\", token);",
							"        console.log(\"Admin token set successfully.\");",
							"    } else if (role === \"host\") {",
							"        pm.collectionVariables.set(\"token_host\", token);",
							"        console.log(\"Host token set successfully.\");",
							"    } else if (role === \"user\") {",
							"        pm.collectionVariables.set(\"token_user\", token);",
							"        console.log(\"User token set successfully.\");",
							"    } else {",
							"        console.warn(\"Unknown role:\", role);",
							"    }",
							"} else {",
							"    console.error(\"Token or role missing in response:\", jsonResponse);",
							"}"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
